        <activity android:name=".FullCalc" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LoginResultActivity" />
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DealActivity" />
        <activity android:name=".InvoiceActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".IntentDemoActivity" />
        <activity android:name=".VibrationActivity" />
        <activity android:name=".SpinnerActivity" />
        <activity android:name=".RadioActivity" />
        <activity android:name=".ImageResourceActivity" />
        <activity android:name=".AlertDialogActivity" />
        <activity android:name=".MultiOptionAlertActivity" />
        <activity android:name=".ToggleSwitchActivity" />
        <activity android:name=".DateActivity" />
        <activity android:name=".OptionMenuActivity" />
        <activity android:name=".SharedPreferenceActivity" />
        <activity android:name=".ProximitySensorActivity" />
