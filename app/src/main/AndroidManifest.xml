        <activity android:name=".WifiActivity" />
        <activity android:name=".DatabaseActivity" />
        <activity android:name=".ViewDatabaseActivity" />
        <activity android:name=".DatabaseDeleteActivity" />
        <activity android:name=".DatabaseUpdateActivity" />
        <activity android:name=".DatabaseView2Activity" />

        <provider
            android:name=".MyContentProvider"
            android:authorities="com.example.amandeep.myfirstapp.customerprovider"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Orientation_Entry_Save" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".MyBroadcastReciever1"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".MyBroadcastReceiver2"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
        </receiver>
    </application>

